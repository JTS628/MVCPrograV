							<form asp-action="OfficerSaveVisit" asp-controller="Security" method="post">
								<div class="row gtr-uniform">
									<div class="col-12">
										<label for="nombre">Nombre</label>
										<input type="text" id="nombre" name="Nombre" placeholder="Nombre del visitante" required />
									</div>

									<div class="col-12">
										<label for="cedula">Cédula</label>
										<input type="number" id="cedula" name="Cedula" placeholder="Cédula del visitante" required />
									</div>
									<div class="col-12">
										<label for="nombre">Correo</label>
										<input type="text" id="correo" name="correo" placeholder="Correo electronico" required />
									</div>

									<div class="col-12">
										<label for="marca">Marca del Vehículo</label>
										<input type="text" id="marca" name="Marca" placeholder="Ej. Toyota, Honda..." required />
									</div>

									<div class="col-12">
										<label for="marca">Modelo del Vehiculo</label>
										<input type="text" id="modelo" name="modelo" placeholder="Ej. Rav4, Santa Fe..." required />
									</div>

									<div class="col-12">
										<label for="color">Color</label>
										<input type="text" id="color" name="Color" placeholder="Color del vehículo" required />
									</div>

									<div class="col-12">
										<label for="placa">Placa</label>
										<input type="text" id="placa" name="Placa" placeholder="Placa del vehículo" required />
									</div>

									<div class="col-12">
										<label for="fecha">Fecha de visita</label>
										<input type="date" id="fecha" name="Fecha" required />
									</div>

									<div class="col-12">
										<ul class="actions">
											<li><input type="submit" class="primary" value="Guardar" /></li>
											<li><a asp-action="Index" asp-controller="Security" class="button">Volver</a></li>
										</ul>
									</div>
								</div>
							</form>
